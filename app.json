{
    "expo": {
        "name": "app-importaciones",
        "slug": "app-importaciones",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/images/icon.png",
        "scheme": "appimportaciones",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/adaptive-icon.png",
                "backgroundColor": "#ffffff",
                "enableProguardInReleaseBuilds": true,
                "extraProguardRules": "-keep class com.google.android.gms.ads.** { *; }"
            },
            "edgeToEdgeEnabled": true,
            "permissions": ["com.google.android.gms.permission.AD_ID"],
            "package": "com.izimport.app"
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/images/favicon.png"
        },
        "plugins": [
            [
                "expo-splash-screen",
                {
                    "image": "./assets/images/splash-icon.png",
                    "imageWidth": 200,
                    "resizeMode": "contain",
                    "backgroundColor": "#ffffff"
                }
            ],
            "expo-tracking-transparency",
            "expo-build-properties",
            [
                "react-native-google-mobile-ads",
                {
                    "androidAppId": "ca-app-pub-9951750194673259~6110385185",
                    "skAdNetworkItems": []
                }
            ]
        ],
        "extra": {
            "eas": {
                "projectId": "de3fe4dd-6547-4259-8fc4-e46f9abc4733"
            }
        }
    }
}
